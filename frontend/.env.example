# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_SOCKET_URL=http://localhost:5000

# AI Configuration
REACT_APP_OPENAI_API_KEY=your-openai-api-key
REACT_APP_GEMINI_API_KEY=your-gemini-api-key

# Video Conferencing
REACT_APP_JITSI_DOMAIN=meet.jit.si
REACT_APP_TWILIO_API_KEY=your-twilio-api-key

# WebRTC Configuration
REACT_APP_TURN_SERVER_URL=turn:your-turn-server.com:3478
REACT_APP_TURN_USERNAME=your-turn-username
REACT_APP_TURN_CREDENTIAL=your-turn-credential

# AI Proctoring Configuration
REACT_APP_FACE_DETECTION_MODEL_URL=/models/face-detection
REACT_APP_PROCTORING_ENABLED=true
REACT_APP_CAMERA_QUALITY=720p

# Feature Flags
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_GAMIFICATION=true
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_VIDEO_CONFERENCING=true

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=your-ga-id

# File Upload
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=.pdf,.doc,.docx,.ppt,.pptx,.jpg,.jpeg,.png,.mp4,.mp3

# App Configuration
REACT_APP_APP_NAME=Excellence Coaching Hub
REACT_APP_APP_VERSION=1.0.0
REACT_APP_SUPPORT_EMAIL=support@excellencecoaching.com
